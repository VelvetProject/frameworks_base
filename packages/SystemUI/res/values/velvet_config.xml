<?xml version="1.0" encoding="utf-8"?>

<resources>

    <!-- Control whether status bar should distinguish HSPAP data icon form HSPA
    data icon on devices -->
    <bool name="config_hspap_data_distinguishable">true</bool>

    <!-- Double tap anywhere in lockscreen makes the device go to sleep -->
    <bool name="config_doubletap_lockscreen_sleep">true</bool>

    <!-- Allow Opa (Pixel navbar) layout -->
    <bool name="config_allowOpaLayout">true</bool>

</resources>
